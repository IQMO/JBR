{
  "name": "@jabbr/backend",
  "version": "1.0.0",
  "description": "Jabbr Trading Bot Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e ../../.env ts-node src/index.ts",
    "build": "tsc",
    "start": "dotenv -e ../../.env node dist/index.js",
    "test": "dotenv -e ../../.env jest",
    "test:unit": "dotenv -e ../../.env jest --selectProjects \"Unit Tests\"",
    "test:integration": "dotenv -e ../../.env jest --selectProjects \"Integration Tests\"",
    "test:e2e": "dotenv -e ../../.env jest --selectProjects \"E2E Tests\"",
    "test:watch": "dotenv -e ../../.env jest --watch",
    "test:coverage": "dotenv -e ../../.env jest --coverage",
    "test:strategy": "dotenv -e ../../.env ts-node scripts/test/test-strategy-framework.ts",
    "backtest": "dotenv -e ../../.env ts-node scripts/backtest/sma-backtest.ts",
    "backtest:fixed": "dotenv -e ../../.env ts-node scripts/backtest/fixed-sma-backtest.ts",
    "test:signals": "dotenv -e ../../.env ts-node scripts/test/test-sma-signals.ts",
    "debug:signals": "dotenv -e ../../.env ts-node scripts/debug/debug-sma-signals.ts",
    "debug:processor": "dotenv -e ../../.env ts-node scripts/debug/debug-sma-processor.ts",
    "debug:simple": "dotenv -e ../../.env ts-node scripts/debug/simple-sma-debug.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@jabbr/shared": "file:../shared",
    "bcryptjs": "^2.4.3",
    "ccxt": "^4.4.92",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ntp-client": "^0.5.3",
    "pg": "^8.16.3",
    "redis": "^4.6.10",
    "winston": "^3.17.0",
    "ws": "^8.14.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.8",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "trading",
    "bot",
    "api",
    "backend"
  ],
  "author": "Jabbr Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
