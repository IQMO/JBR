{
  "name": "jabbr-trading-bot-platform",
  "version": "1.0.0",
  "description": "A fully modular, TypeScript-first trading bot platform with WebSocket-first architecture",
  "private": true,
  "workspaces": [
    "packages/backend",
    "packages/frontend",
    "packages/shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "dotenv -e .env tsx packages/backend/src/index.ts",
    "dev:frontend": "cd packages/frontend && next dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd packages/frontend && next build",
    "start:frontend": "cd packages/frontend && next start",
    "test": "jest",
    "test:unit": "jest --selectProjects \"*Unit Tests\"",
    "test:integration": "jest --selectProjects \"Integration Tests\"",
    "test:e2e": "jest --selectProjects \"E2E Tests\"",
    "test:backend": "jest --selectProjects \"Backend*\"",
    "test:frontend": "jest --selectProjects \"Frontend*\"",
    "test:shared": "jest --selectProjects \"Shared*\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "backtest": "dotenv -e .env tsx packages/backend/scripts/backtest/sma-backtest.ts",
    "backtest:fixed": "dotenv -e .env tsx packages/backend/scripts/backtest/fixed-sma-backtest.ts",
    "debug:signals": "dotenv -e .env tsx packages/backend/scripts/debug/debug-sma-signals.ts",
    "debug:processor": "dotenv -e .env tsx packages/backend/scripts/debug/debug-sma-processor.ts",
    "debug:simple": "dotenv -e .env tsx packages/backend/scripts/debug/simple-sma-debug.ts",
    "debug:compare": "dotenv -e .env tsx packages/backend/scripts/debug/compare-sma-processors.ts",
    "test:db": "dotenv -e .env tsx packages/backend/scripts/test-db-connection.ts",
    "test:websocket": "dotenv -e .env tsx packages/backend/scripts/test-real-websocket.ts",
    "test:realdata": "dotenv -e .env tsx packages/backend/scripts/test-real-data.ts",
    "test:bybit": "dotenv -e .env tsx packages/backend/scripts/test-bybit-formats.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality:check": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "clean": "rimraf packages/frontend/.next packages/frontend/out coverage .jest-cache",
    "duplication:check": "jscpd --config .jscpd.json",
    "duplication:analyze": "npx tsx scripts/analysis/duplication-analyzer.ts",
    "production:check": "npx tsx scripts/monitoring/production-violations-analyzer.ts",
    "validate:docs": "npx tsx scripts/validate-documentation.ts",
    "validate:docs:precise": "npx tsx scripts/validate-documentation-precise.ts",
    "validate:post-implementation": "npx tsx scripts/monitoring/post-implementation-validator.ts",
    "validate:naming": "npx tsx scripts/utilities/naming-validator.ts",
    "validate:frontend": "npx tsx scripts/testing/validation/frontend-comprehensive-test.ts",
    "detect:duplicates": "npx tsx scripts/analysis/duplicate-method-detector.ts",
    "detect:duplicate-methods": "npx tsx scripts/analysis/duplicate-method-detector.ts",
    "validate:docs:comprehensive": "npx tsx scripts/monitoring/comprehensive-doc-validator.ts",
    "validate:docs:legacy": "npx tsx scripts/validate-documentation.ts",
    "quality:analyze": "npx tsx scripts/analysis/analyze.ts",
    "security:check": "eslint . --ext .ts,.tsx,.js,.jsx --no-eslintrc --config .eslintrc.security.js",
    "analyze:all": "npm run quality:check && npm run duplication:check && npm run test:coverage",
    "scripts": "npx tsx scripts/orchestrator.ts",
    "scripts:list": "npx tsx scripts/orchestrator.ts list",
    "scripts:status": "npx tsx scripts/orchestrator.ts status",
    "workflow:analysis": "npx tsx scripts/orchestrator.ts workflow comprehensive-analysis",
    "workflow:production": "npx tsx scripts/orchestrator.ts workflow production-ready",
    "workflow:health": "npx tsx scripts/orchestrator.ts workflow quick-health",
    "workflow:testing": "npx tsx scripts/orchestrator.ts workflow system-testing",
    "workflow:backtest": "npx tsx scripts/orchestrator.ts workflow strategy-testing",
    "workflow:full": "npx tsx scripts/orchestrator.ts workflow full-validation",
    "test:performance": "npx tsx scripts/orchestrator.ts test --performance",
    "test:system": "npx tsx scripts/orchestrator.ts test --integration",
    "test:backtest": "npx tsx scripts/orchestrator.ts test --backtest",
    "install:all": "npm install"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^30.0.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jscpd": "^4.0.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "trading-bot",
    "cryptocurrency",
    "websocket",
    "typescript",
    "monorepo",
    "real-time",
    "bybit"
  ],
  "author": "Jabbr Trading Platform",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@types/multer": "^2.0.0",
    "multer": "^2.0.1",
    "react-hook-form": "^7.59.0",
    "zod": "^3.25.67"
  }
}
